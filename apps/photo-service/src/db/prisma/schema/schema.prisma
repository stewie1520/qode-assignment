datasource db {
  provider = "postgresql"
}

generator client {
  provider     = "prisma-client"
  output       = "../generated"
  moduleFormat = "esm"
  runtime      = "nodejs"
}

model Photo {
  id String @id @default(cuid())

  userId  String
  comment String

  filePath    String
  contentType String
  size        Int

  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt
}
